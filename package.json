{
  "name": "nuxt-ssr-template",
  "version": "1.0.0",
  "description": "nuxt-ssr-template",
  "author": "2874539950@qq.com",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev-hk": "cross-env NODE_ENV=development LANGUAGE=hk nodemon server/index.js --watch server",
    "dev-en": "cross-env NODE_ENV=development LANGUAGE=en nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start-hk": "cross-env NODE_ENV=production LANGUAGE=hk node server/index.js",
    "start-en": "cross-env NODE_ENV=production LANGUAGE=en node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "local": "cd ./dist && live-server"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/style-resources": "^1.0.0",
    "clipboard": "^2.0.4",
    "cookie-universal-nuxt": "^2.0.19",
    "cross-env": "^5.2.0",
    "crypto-js": "^4.0.0",
    "express": "^4.16.4",
    "js-base64": "^2.5.2",
    "js-cookie": "^2.2.1",
    "nuxt": "^2.0.0",
    "qs": "^6.9.1",
    "view-design": "^4.1.3",
    "vue-i18n": "^8.10.0",
    "vue2-toast": "^2.1.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "node-sass": "^4.14.1",
    "nodemon": "^1.18.9",
    "sass": "^1.26.7",
    "sass-loader": "^7.1.0"
  }
